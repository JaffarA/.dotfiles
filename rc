
# >>> jaf >>>

if [ -f /usr/bin/zsh ]
then
        ZSH_THEME='ys'
fi

if [ -f /usr/bin/ytop ] || [ -f /usr/local/bin/ytop ]
then
        alias ytop='ytop -m -s -c vice'
fi

if [ -f /usr/bin/xclip ] || [ -f /usr/local/bin/xclip ]
then
        alias c='xclip -selection clipboard'
        alias v='xclip -o'
fi

if [ -f /usr/bin/neofetch ] || [ -f /usr/local/bin/neofetch ]
then
        neofetch
fi

# <<< jaf <<<
